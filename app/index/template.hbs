<div class="leaflet-container">

    {{#leaflet-map lat=model.bounds.northeast.lat lng=model.bounds.northeast.lng zoom=zoom scrollWheelZoom=false onMoveend=(action "updateCenter")}}
      - {{google-route route=model}} - {{tile-layer url="http://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"}} -
      {{/leaflet-map}}
  </div>

<div class="section">
      <form {{action 'submit' on='submit'}} class="destination">
        <div class="input-container__route">
          <div class="input-container__input">
          {{input class="destination__input" value=formValues.year changeset=formValues placeholder="Origin"}}
          </div>
          <div class="input-container__input">
            {{input class="destination__input" value=formValues.make changeset=formValues placeholder="Destination"}}
          </div>
          <div class="input-container__input">
            {{input class="destination__input" value=formValues.make changeset=formValues placeholder="Fuel Price"}}
          </div>
          <div class="input-container__input">
            <button class="destination-btn" {{action 'submit'}}>Go</button>
          </div>
        </div>
      </form>
